
    <!DOCTYPE html>
    <html>
        <head>
            <meta charset="utf-8" />
            <meta name="viewport" content="width=device-width, initial-scale=1.0" />
            <title>@ViewData["Title"]</title>

            <environment names="Development">

                <link href="~/lib/github/twbs/bootstrap@3.3.6/css/bootstrap.css" rel="stylesheet" />
                <link href="~/lib/npm/font-awesome@4.6.3/css/font-awesome.min.css" rel="stylesheet" />
                <link href="~/css/styles.css" rel="stylesheet" />

                <link href="https://getbootstrap.com/examples/dashboard/dashboard.css" rel="stylesheet" />
            </environment>
            <environment names="Staging,Production">
                <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.5/css/bootstrap.min.css"
                    asp-fallback-href="~/lib/github/twbs/bootstrap@3.3.6/css/bootstrap.css"
                    asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
            
                <link href="https://getbootstrap.com/examples/dashboard/dashboard.css" rel="stylesheet" />
                <link href="~/dist/styles.css" rel="stylesheet" />
            </environment>
           
        </head>
        <body>
            @RenderBody()



            @RenderSection("scripts", required: false)
        </body>
    </html>
