{

  "compileOnSave": false,
  "compilerOptions": {
  
    "sourceMap": true,
    "target": "es5",
    "module": "amd",
    "moduleResolution": "classic",
    "declaration": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "noResolve": true,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "wwwroot/dist"
  },

  "exclude": [
    "node_modules"
  ]
}
